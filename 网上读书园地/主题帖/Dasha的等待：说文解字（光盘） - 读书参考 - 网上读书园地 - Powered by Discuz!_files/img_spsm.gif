<script type="text/javascript">
  $(function(){
    //栏目导航高亮
    var innercode=$("#CatalogInnerCode").val();
    //手机端头部的栏目名
        $('.nav-ul a').each(function(){
          var catalogIDCode = $(this).attr('id')+'';
          catalogIDCode = catalogIDCode.substr(0,6);          
          if(catalogIDCode == innercode){
            $('#catalogName').html($(this).html());
          }
        })
    if(innercode){
      var $node=$("#nv ul li.on");
      var $a=$("#nv ul li:not(.on) a");
      for(var i=0;i<$a.length;i++){
        var ids = $a[i].id.split("|");
        for(var index = 0;index < ids.length;index++){
          if(ids[index]!="" && innercode.startsWith(ids[index])){
            $node.removeClass("on");
            $($a[i]).parent("li").addClass("on");
            break;
          }
        }
      }
    } 
    var $b=$("#fwzqli li");
    for(var i=0;i<$b.length;i++){
      if($($b[i]).attr("class")=="on"){
        //$($b[i]).removeClass("on");
        $("#fwzqli").parent().addClass("on");
      }
    }
    //头部搜索
    $(".topBtn").click(function(){
      $(".soForm").slideToggle();
    })
  });
</script>
